{
  "compilerOptions": {
    "declaration": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "sourceMap": true,
    "outDir": "./build",
    "importHelpers": true,
    "lib": [
      "es5",
      "es2015.promise",
      "dom"
    ],
    "types": []
  },
  "include": [
    "src/index.ts"
  ]
}